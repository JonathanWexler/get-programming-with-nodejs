  <div class="container">
    <h2>Courses Table</h2>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Description</th>
          <th>Cost</th>
          <th>Max Students</th>
          <th>Edit</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        <% courses.forEach(course => { %>
          <tr>
          <td> <a href=<%= `/courses/${course._id}` %>><%= course.name%></a></td>
          <td><%= course.description%></td>
          <td><%= course.cost%></td>
          <td><%= course.maxStudents%></td>
          <td> <a href=<%= `courses/${course._id}/edit` %>>Edit</a></td>
          <td>
          <a href=<%= `courses/${course._id}/delete?_method=DELETE` %> onclick="return confirm('Are you sure you want to delete this record?')" a>Delete</a>
          </td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
