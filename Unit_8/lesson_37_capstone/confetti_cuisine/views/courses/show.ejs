<h1 class="center">Course Data for <%= course.title %></h1>

<table class="table">
  <tr>
    <th>Title</th>
    <td><%= course.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= course.description%></td>
  </tr>
  <tr>
    <th>Max Students</th>
    <td><%= course.maxStudents%></td>
  </tr>
  <tr>
    <th>Cost</th>
    <td>$<%= course.cost%></td>
  </tr>
</table>
