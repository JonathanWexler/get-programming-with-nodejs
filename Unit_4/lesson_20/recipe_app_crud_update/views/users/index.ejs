<h2 class="center">Users Table</h2>
 <table class="table">
   <thead>
     <tr>
       <th>Name</th>
       <th>Email</th>
       <th>Zip Code</th>
       <th>Edit</th>
     </tr>
   </thead>
   <tbody>
     <% users.forEach(user => { %>
     <tr>
      <td>
      <a href=<%= `/users/${user._id}` %>>
        <%= user.fullName %>
      </a>
      </td>
       <td><%= user.email %></td>
       <td><%= user.zipCode %></td>
       <td>
        <a href=<%= `users/${user._id}/edit` %>>
          Edit
        </a>
      </td>
     </tr>
     <% }); %>
   </tbody>
 </table>
