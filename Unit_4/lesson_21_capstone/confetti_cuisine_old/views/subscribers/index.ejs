<div class="container">
  <h2>Subscribers Table</h2>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Zip Code</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% subscribers.forEach(subscriber => { %>
        <tr>
        <td> <a href=<%= `/subscribers/${subscriber._id}` %>><%= subscriber.name%></a></td>
        <td><%= subscriber.email%></td>
        <td><%= subscriber.zipCode%></td>
        <td> <a href=<%= `subscribers/${subscriber._id}/edit` %>>Edit</a></td>
        <td>
        <a href=<%= `subscribers/${subscriber._id}/delete?_method=DELETE` %> onclick="return confirm('Are you sure you want to delete this record?')" a>Delete</a>
        </td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
